pipeline {
  agent any
  stages {
    stage('Collect new data') {
      steps {
             echo 'hello.py'
      }
    }
    stage('Validate new data') {
      steps {
        echo 'hello.py'
      }
    }
    stage('Deploy new Pipeline') {
      steps {
        echo 'hello.py'
      }
    }
  }
}
